  <!-- JS: nav + show-more logic -->
  <script type='text/javascript'><![CDATA[
  (function(){
    var navbar = document.getElementById('navbar');
    var navLinksEl = document.getElementById('navLinks');

    // set year in footer
    var yearEl = document.getElementById('year');
    if(yearEl) yearEl.textContent = new Date().getFullYear();

    // smooth scroll for anchor links + close mobile menu on click
    var anchorLinks = document.querySelectorAll('a[href^="#"]');
    for (var i = 0; i < anchorLinks.length; i++) {
      anchorLinks[i].addEventListener('click', function(e){
        var href = this.getAttribute('href');
        if(!href || href === '#') return;
        var target = document.querySelector(href);
        if(target){
          e.preventDefault();
          target.scrollIntoView({behavior:'smooth', block:'start'});
          if(navLinksEl && navLinksEl.classList.contains('active')){
            navLinksEl.classList.remove('active');
          }
        }
      });
    }

    // active nav highlight
    var sections = document.querySelectorAll('section');
    var navItems = document.querySelectorAll('.nav-link');

    function updateActiveLink(){
      if(!sections.length) return;
      var idx = sections.length - 1;
      for(var i=0;i<sections.length;i++){
        var rect = sections[i].getBoundingClientRect();
        if(rect.top <= 120) idx = i;
      }
      for (var j=0; j<navItems.length; j++) {
        navItems[j].classList.remove('active');
      }
      var id = (sections[idx] && sections[idx].id) ? sections[idx].id : 'home';
      var activeLink = document.querySelector('.nav-link[href="#'+id+'"]');
      if(activeLink) activeLink.classList.add('active');
    }

    // auto-hide navbar on scroll down, show on scroll up
    var lastScrollY = window.scrollY || window.pageYOffset || 0;
    var scrollThreshold = 8;

    function handleScroll(){
      if(!navbar) return;
      var currentY = window.scrollY || window.pageYOffset || 0;

      if(currentY > 20) navbar.classList.add('scrolled');
      else navbar.classList.remove('scrolled');

      if(navLinksEl && navLinksEl.classList.contains('active') &&
         Math.abs(currentY - lastScrollY) > 10){
        navLinksEl.classList.remove('active');
      }

      if(
        currentY > lastScrollY + scrollThreshold &&
        currentY > 60 &&
        !(navLinksEl && navLinksEl.classList.contains('active'))
      ){
        navbar.classList.add('nav-hidden');
      } else if(currentY < lastScrollY - scrollThreshold || currentY <= 60){
        navbar.classList.remove('nav-hidden');
      }

      lastScrollY = currentY;
      updateActiveLink();
    }

    handleScroll();
    window.addEventListener('scroll', handleScroll);

    // ---------- SHOW MORE / SHOW LESS TOGGLES (WORKING EVERYWHERE) ----------

    function setupToggle(btnId, containerSelector) {
      var btn = document.getElementById(btnId);
      if (!btn) return;
      var container = document.querySelector(containerSelector);
      if (!container) return;

      btn.addEventListener('click', function(){
        var extras = container.querySelectorAll('[data-extra]');
        if (!extras.length) return;

        var anyHidden = false;
        for (var i = 0; i < extras.length; i++) {
          if (extras[i].classList.contains('hidden')) {
            anyHidden = true;
            break;
          }
        }

        for (var j = 0; j < extras.length; j++) {
          if (anyHidden) extras[j].classList.remove('hidden');
          else extras[j].classList.add('hidden');
        }

        btn.textContent = anyHidden ? 'Show less' : 'Show more';
      });
    }

    function hideToggleIfNone(btnId, containerSelector) {
      var btn = document.getElementById(btnId);
      if (!btn) return;
      var container = document.querySelector(containerSelector);
      if (!container) return;
      var extras = container.querySelectorAll('[data-extra]');
      if (!extras || extras.length === 0) {
        btn.style.display = 'none';
      }
    }

    setupToggle('projectsToggle', '#projectsGrid');
    setupToggle('videosToggle', '#videosGrid');
    setupToggle('galleryToggle', '#galleryGrid');
    setupToggle('certsToggle', '#certGrid');

    hideToggleIfNone('projectsToggle', '#projectsGrid');
    hideToggleIfNone('videosToggle', '#videosGrid');
    hideToggleIfNone('galleryToggle', '#galleryGrid');
    hideToggleIfNone('certsToggle', '#certGrid');

  })();
  ]]></script>

